{
  "name": "my-portfolio",
  "version": "1.0.0",
  "description": "My portfolio website for netlify.",
  "author": "namoo-san",
  "private": true,
  "config": {
    "nuxt": {
      "host": "0.0.0.0",
      "port": "3000"
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
