    "@nuxtjs/pwa": "3.3.5",
    "axios": "0.21.1",
    "babel-eslint": "10.0.3",
    "cross-env": "6.0.3",
    "eslint": "7.20.0",
    "express": "4.17.1",
    "nuxt": "2.15.2",
    "prettier": "2.2.1"
  },
  "devDependencies": {
    "nodemon": "2.0.7"
  }
}
